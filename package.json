{
  "name": "remark-github-alerts",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@8.11.0",
  "description": "Support GitHub-style alerts for remark",
  "author": "Stephen Zhou <hi@hyoban.cc>",
  "license": "MIT",
  "homepage": "https://github.com/hyoban/remark-github-alerts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/remark-github-alerts.git"
  },
  "bugs": "https://github.com/hyoban/remark-github-alerts/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles/*": "./styles/*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --max-warnings 0 .",
    "prepublishOnly": "nr build",
    "release": "changelogen --clean --output && prettier --write CHANGELOG.md && bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "simple-git-hooks && pnpm build"
  },
  "dependencies": {
    "unist-util-visit": "^5.0.0"
  },
  "peerDependencies": {
    "@types/mdast": "^4.0.0",
    "unified": "^11.0.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.6",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.10.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "bumpp": "^9.2.0",
    "changelogen": "^0.5.5",
    "dedent": "^1.5.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "esno": "^4.0.0",
    "pnpm": "^8.11.0",
    "prettier": "^3.1.0",
    "rehype-format": "^5.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-mdc": "^2.1.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vite": "^5.0.4",
    "vitest": "^1.0.0-beta.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test:run && pnpm build"
  }
}
